.custom-input {
  width: 260px;
  height: 40px;
  border-bottom: 1px solid yellow;
  margin-bottom: 16px;
  color: #c6c6c6;
  padding-left: 10px;
  padding-right: 10px;
}

.custom-input::placeholder {
  color: #c6c6c6;
}

.custom-button {
  width: 260px;
  height: 52px;
  border-radius: 6px;
  background-color: #ffd700;
  color: #000;
  font-size: 18px;
  font-weight: 800;
  margin-top: 16px;
}
